       }
        /* configurações gerais */

       body  {
        background: #c4e9bd;
        font-family: "Ubuntu" !important;
        margin: 0px;
        padding: 0px;
        color: #696969;
        width: 100%;
       }

       .main{
           width: 100%;
           display: flex;
           flex-direction: column;
           padding: 0 !important;
           margin: 0 !important;
       }

       div.menu{
           padding: 0 !important;
       }

       .menu{
           justify-content: start;
           height: 100%;
           background-color: #f1f1f1;
       }

       #ctl00_Menu1{
           height: 100%;
           width: fit-content;
           position: fixed;
           background-color: #f1f1f1;
           display: flex;
       }

       img#eo_ele_21, img#eo_ele_130, img#eo_ele_138, img#eo_ele_144, img#eo_ele_149, img#eo_ele_156, img#eo_ele_164, img#eo_ele_110, img#eo_ele_118, img#eo_ele_57,
       img#eo_ele_62, img#eo_ele_67, img#eo_ele_78, img#eo_ele_91, img#eo_ele_14, img#eo_ele_146, img#eo_ele_152, img#eo_ele_157, img#eo_ele_125, img#eo_ele_165, 
